<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="cn.itcast.domain" >

	<class name="Manager" table="manager" >
		
		<id name="manager_id"  >
			<generator class="native"></generator>
		</id>
	
		<property name="manager_num" column="manager_num" unique="true"></property>
		<property name="manager_name" column="manager_name" ></property>
		<property name="enter_time" column="enter_time" ></property>
		<property name="email" column="email" ></property>
		<property name="sex" column="sex" ></property>
		<property name="tel" column="tel" ></property>
		
	</class>
</hibernate-mapping>